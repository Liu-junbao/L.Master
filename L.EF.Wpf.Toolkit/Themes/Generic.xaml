<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:System.Windows">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/L.EF.Wpf.Toolkit;component/Themes/ST.EFDataGrid.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style BasedOn="{StaticResource st.EFValueEditor}" TargetType="{x:Type local:EFValueEditor}" />
    <Style BasedOn="{StaticResource st.EFOperator}" TargetType="{x:Type local:EFOperator}" />
    <Style BasedOn="{StaticResource st.EFDataGrid}" TargetType="local:EFDataGrid" />
    <Style BasedOn="{StaticResource st.EFDataGridBar}" TargetType="local:EFDataGridBar" />
    <Style BasedOn="{StaticResource st.EFDataBox}" TargetType="{x:Type local:EFDataBox}" />
</ResourceDictionary>
